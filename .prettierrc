{
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid",
    "overrides": [
      {
        "files": "*.ejs",
        "options": {
          "parser": "html",
          "plugins": ["prettier-plugin-ejs"]
        }
      }
    ]
  }
  